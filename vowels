#include <stdio.h>

int main()
{
    char ch;
    printf("enter a alphabet");
    scanf("%c",&ch);
    switch(ch)
    {
        case 'a':'A':
        print("vowel");
        break;
        
        case 'i':'I':
        print("vowel");
        break;
        
        case 'e': 'E':
        print("vowel");
        break;
        
        case 'o': 'O':
        print("vowel");
        break;
        
        case 'u':'U':
        print("vowel");
        break;
        
        default:
        printf("consonant");
        
    }

    return 0;
}

